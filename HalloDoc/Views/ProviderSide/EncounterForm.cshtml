ere" id="floatingTextarea2" style="height: 80px"></textarea>                <label>Other</label>                <span asp-validation-for="Other" class="text-danger"></span>            </div>        </div>        <div class="p-2 col-sm-6 col-12">            <div class="form-floating">                <textarea asp-for="Diagnosis" class="form-control fourthedit" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 80px"></textarea>                <label>Diagnosis</label>                <span asp-validation-for="Diagnosis" class="text-danger"></span>            </div>        </div>        <div class="p-2 col-sm-6 col-12">            <div class="form-floating">                <textarea asp-for="TreatmentPlan" class="form-control fourthedit" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 80px"></textarea>                <label>Treat Plan</label>                <span asp-validation-for="TreatmentPlan" class="text-danger"></span>            </div>        </div>        <div class="p-2 col-sm-6 col-12">            <div class="form-floating">                <textarea asp-for="MedicationsDispended" class="form-control fourthedit" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 80px"></textarea>                <label>Medications Dispensed</label>                <span asp-validation-for="MedicationsDispended" class="text-danger"></span>            </div>        </div>        <div class="p-2 col-sm-6 col-12">            <div class="form-floating">                <textarea asp-for="Procedure" class="form-control fourthedit" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 80px"></textarea>                <label>Procedures</label>                <span asp-validation-for="Procedure" class="text-danger"></span>            </div>        </div>        <div class="p-2 col-12">            <div class="form-floating">                <textarea asp-for="Follow